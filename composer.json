  {  
      "name": "fancyecommerce/fecshop-app-advanced",  
      "description": "fancyecommerce yii2 fecshop application advanced",  
      "keywords": [  
        "yii2",  
        "fecshop" , 
        "fancyecommerce" ,
		"advanced"
      ],  
      "homepage": "https://github.com/fancyecommerce/yii2_fecshop_app_advanced",  
      "type": "yii2-extension",  
      "license": "MIT",  
      "support": {  
        "source": "https://github.com/fancyecommerce/yii2_fecshop_app_advanced"  
      },  
      "authors": [  
        {  
          "name": "terry",  
          "email": "2358269014@qq.com"  
        }  
      ],  
	"minimum-stability": "stable", 
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "fancyecommerce/fec_admin": "1.3.5.5",
        "fancyecommerce/fecshop": "1.0.0.4",
        "skeeks/yii2-assets-auto-compress": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
